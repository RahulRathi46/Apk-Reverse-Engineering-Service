{
    "name": "ApkDecompile/slim",
    "description": "A Slim Framework Backed WebApp",
    "keywords": ["microframework", "rest", "router", "psr7" , "APK", "DECODE"],
    "homepage": "",
    "license": "COPYRIGHTED",
    "authors": [
        {
            "name": "Rahul Rathi",
            "email": "VanGiex.RR@Gmail.Com",
            "homepage": "https://www.ApkDecompile.com"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "slim/slim": "^3.1",
        "slim/php-view": "^2.0",
        "monolog/monolog": "^1.17",
        "ext-json": "*",
        "ext-zip": "*"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8 < 6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout" : 0
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }

}
